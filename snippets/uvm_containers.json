      "\t/*  VARIABLES  */\n\n",
      "\tfunction new(string name=\"$1\");",
      "\t\tsuper.new(name);",
      "\tendfunction: new\n\n",
      "\t$0",
      "endclass: $1"
    ],
    "description": "UVM parametrized object class template"
  },
