  "UVM_object": {
    "prefix": "uvm_class_object",
    "body": [
      "class ${1:<class_name>} extends ${2:<base_class>};",
      "\t`uvm_object_utils($1);\n",
      "\t/*  VARIABLES  */\n\n",
      "\tfunction new(string name=\"$1\");",
      "\t\tsuper.new(name);",
      "\tendfunction: new\n\n",
      "\t$0",
      "endclass: $1"
    ],
    "description": "UVM object class template"
  },
